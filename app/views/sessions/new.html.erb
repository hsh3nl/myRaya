<%= form_with scope: :user, local: true  do |form| %>
  <p>
    <%= form.label :email_address %><br>
    <%= form.text_field :email %>
  </p>    

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </p>  
 
  <p>
    <%= form.submit 'Sign In', class:' btn btn-sm btn-outline-success'%>
  </p>
<% end %>